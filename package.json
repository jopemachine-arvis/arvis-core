{
  "name": "arvis-core",
  "version": "0.1.25",
  "description": "Core module of Arvis",
  "main": "dist/index.js",
  "files": [
    "dist/**",
    "execa/**/*.js",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "test": "jest --verbose ./__test__/unit",
    "build": "tsc",
    "start": "npm run build && dist/cli.js",
    "lint": "tslint --project tsconfig.json --fix",
    "rebuild": "rm -rf node_modules && npm i && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jopemachine/arvis-core.git"
  },
  "keywords": [
    "arvis"
  ],
  "author": {
    "name": "jopemachine",
    "email": "jopemachine@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "bugs": {
    "url": "https://github.com/jopemachine/arvis-core/issues"
  },
  "homepage": "https://github.com/jopemachine/arvis-core#readme",
  "devDependencies": {
    "@types/archiver": "^5.1.0",
    "@types/chalk": "^2.2.0",
    "@types/chmodr": "^1.0.0",
    "@types/encodeurl": "^1.0.0",
    "@types/fs-extra": "^9.0.8",
    "@types/is-url": "^1.2.29",
    "@types/lodash": "^4.14.168",
    "@types/node": "^14.14.35",
    "@types/parse-json": "^4.0.0",
    "@types/rimraf": "^3.0.0",
    "@types/semver": "^7.3.6",
    "@types/string-similarity": "^4.0.0",
    "@types/unzipper": "^0.10.3",
    "@types/uuid": "^8.3.0",
    "husky": "^4.2.5",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "sleep-synchronously": "^2.0.0",
    "ts-jest": "^26.5.6",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "alpha-sort": "^4.0.0",
    "archiver": "^5.3.0",
    "arvis-extension-validator": "^0.1.0",
    "chalk": "^4.1.0",
    "chmodr": "^1.2.0",
    "clipboardy": "^2.3.0",
    "conf": "^10.0.1",
    "encodeurl": "^1.0.2",
    "env-paths": "^2.2.1",
    "fs-extra": "^9.1.0",
    "is-promise": "^4.0.0",
    "is-url": "^1.2.4",
    "lodash": "^4.17.21",
    "open": "^8.0.4",
    "p-cancelable": "^2.1.1",
    "parse-json": "^5.2.0",
    "path-exists": "^4.0.0",
    "readdirp": "^3.6.0",
    "rimraf": "^3.0.2",
    "semver": "^7.3.5",
    "string-similarity": "^4.0.4",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2",
    "xml-js": "^1.6.11"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": true
      }
    },
    "verbose": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test && npm run lint",
      "pre-push": "npm run test && npm run lint"
    }
  }
}
