{
    "$schema": "some_schema_file_path",
    "creator": "jopemachine",
    "name": "mock-workflow",
    "enabled": true,
    "bundleId": "arvis-mock-workflow",
    "commands": [
        {
            "type": "keyword",
            "command": "createFile",
            "title": "Test 1 - keyword-script",
            "subtitle": "",
            "withspace": false,
            "argType": "no",
            "action": [
                {
                    "modifiers": "normal",
                    "type": "script",
                    "script": "node createFile.js"
                }
            ]
        },
        {
            "type": "scriptfilter",
            "command": "scriptFilter",
            "title": "Test 2 - scriptfilter + if",
            "scriptFilter": "node scriptfilter.js {query}",
            "runningSubtext": "Searching...",
            "withspace": true,
            "argType": "optional",
            "action": [
                {
                    "modifiers": "normal",
                    "type": "args",
                    "arg": "{query}",
                    "action": [
                        {
                            "modifiers": "normal",
                            "type": "cond",
                            "if": {
                                "cond": "5 <= 10",
                                "action": {
                                    "then": [
                                        {
                                            "modifiers": "normal",
                                            "type": "script",
                                            "script": "node createFile.js {query}"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            ]
        },
        {
            "type": "scriptfilter",
            "command": "scriptFilterOverlay",
            "title": "Test 3 - scriptfilter + scriptfilter",
            "scriptFilter": "node scriptfilter.js {query}",
            "runningSubtext": "Searching...",
            "withspace": true,
            "argType": "optional",
            "action": [
                {
                    "type": "scriptfilter",
                    "arg": "{query}",
                    "scriptFilter": "node scriptfilter.js {query}",
                    "runningSubtext": "Searching...",
                    "withspace": true,
                    "action": [
                        {
                            "type": "scriptfilter",
                            "arg": "{query}",
                            "scriptFilter": "node scriptfilter.js {query}",
                            "runningSubtext": "Searching...",
                            "withspace": true,
                            "action": [
                                {
                                    "modifiers": "normal",
                                    "type": "script",
                                    "script": "node createFile.js"
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "keyword",
            "command": "multiple",
            "title": "Test 4 - muitiple actions",
            "subtitle": "",
            "withspace": false,
            "argType": "no",
            "action": [
                {
                    "modifiers": "normal",
                    "type": "script",
                    "script": "node createFile.js 'abc'",
                    "action": [
                        {
                            "modifiers": "normal",
                            "type": "notification",
                            "title": "File created"
                        },
                        {
                            "modifiers": "normal",
                            "type": "open",
                            "title": "https://www.google.co.kr/"
                        }
                    ]
                },
                {
                    "modifiers": "normal",
                    "type": "clipboard",
                    "text": "def",
                    "action": [
                        {
                            "modifiers": "normal",
                            "type": "notification",
                            "title": "Clipboard copied"
                        }
                    ]
                }
            ]
        }
    ]
}